{
  "name": "youtube-downloader-yt-dlp-nextjs-auto-cookies",
  "version": "5.2.0",
  "description": "Production-ready YouTube downloader with timeout fix, relaxed validation, auto-fallback formats. Features auto-fetched cookies, serverless-optimized (120s), server proxy downloads, responsive UI, admin dashboard, and SSE progress.",
  "private": true,
  "author": "YT Downloader Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youtube-downloader-yt-dlp-nextjs-auto-cookies"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "node scripts/download-ytdlp.js",
    "docker:build": "docker build -t youtube-downloader:latest .",
    "docker:run": "docker run -p 3000:3000 -v yt-downloader-data:/data youtube-downloader:latest",
    "docker:push": "docker push yourusername/youtube-downloader:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "db:init": "node -e \"require('./lib/db').initializeDatabase()\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.10.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.4",
    "formidable": "^3.5.1",
    "framer-motion": "^10.18.0",
    "jose": "^5.10.0",
    "lodash": "^4.17.21",
    "next": "14.2.15",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "winston": "^3.11.0",
    "yt-dlp-wrap": "^2.3.12",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.12.24",
    "drizzle-kit": "^0.30.6",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
